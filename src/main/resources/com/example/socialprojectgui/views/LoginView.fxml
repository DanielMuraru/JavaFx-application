<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialprojectgui.controller.LoginController">
   <children>
      <VBox fx:id="layout" layoutX="164.0" layoutY="69.0" prefHeight="200.0" prefWidth="181.0">
         <children>
            <Label fx:id="emailLabel" text="Email" />
            <TextField fx:id="emailText" />
            <Label fx:id="passLabel" text="Password" />
            <PasswordField fx:id="passwordText" />
            <Button fx:id="loginButton" alignment="CENTER" layoutX="50.0" layoutY="20.0" mnemonicParsing="false" onAction="#determineUser" prefHeight="28.0" prefWidth="66.0" text="Login" />
            
         </children>
      </VBox>
   </children>
</AnchorPane>
